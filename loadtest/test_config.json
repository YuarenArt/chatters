{
  "host": "http://localhost:8080",
  "requests_per_minute": 120,
  "users": 10000,
  "spawn_rate": 50,
  "websocket_message_rate": 10,
  "max_rooms": 1000,
  "max_users_per_room": 1000,
  "endpoints": [
    {
      "method": "GET",
      "path": "/api/health",
      "name": "health_check",
      "weight": 1
    },
    {
      "method": "POST",
      "path": "/api/rooms",
      "name": "create_room",
      "weight": 3,
      "body": {
        "password": "testpass123"
      }
    },
    {
      "method": "POST",
      "path": "/api/rooms",
      "name": "create_room_no_password",
      "weight": 3,
      "body": {}
    },
    {
      "method": "GET",
      "path": "/api/rooms/{room_id}",
      "name": "get_room_info",
      "weight": 15
    },
    {
      "method": "POST",
      "path": "/api/rooms/{room_id}/validate-password",
      "name": "validate_password",
      "weight": 5,
      "body": {
        "password": "testpass123"
      }
    },
    {
      "method": "PUT",
      "path": "/api/rooms/{room_id}/password",
      "name": "change_password",
      "weight": 2,
      "headers": {
        "Authorization": "{host_token}"
      },
      "body": {
        "new_password": "newpass456"
      }
    },
    {
      "method": "DELETE",
      "path": "/api/rooms/{room_id}",
      "name": "delete_room",
      "weight": 1,
      "headers": {
        "Authorization": "{host_token}"
      }
    },
    {
      "method": "WEBSOCKET",
      "path": "/ws/{room_id}",
      "name": "websocket_chat",
      "weight": 40,
      "username": "TestUser_{id}",
      "password": "testpass123",
      "host_token": "{host_token}",
      "messages": [
        {
          "type": "chat",
          "data": {
            "text": "Hello, room {room_id}!"
          }
        },
        {
          "type": "chat",
          "data": {
            "text": "Test message {id}"
          }
        }
      ]
    },
    {
      "method": "WEBSOCKET",
      "path": "/ws/{room_id}",
      "name": "websocket_chat_no_password",
      "weight": 15,
      "username": "TestUser_{id}",
      "messages": [
        {
          "type": "chat",
          "data": {
            "text": "Hello from public room!"
          }
        }
      ]
    }
  ]
}
