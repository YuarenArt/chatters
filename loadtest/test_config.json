{
  "host": "http://localhost:8080",
  "requests_per_minute": 120,
  "users": 1000,
  "spawn_rate": 50,
  "websocket_message_rate": 10,
  "max_rooms": 10,
  "max_users_per_room": 100,
  "endpoints": [
    {
      "method": "GET",
      "path": "/api/health",
      "name": "health_check",
      "weight": 2
    },
    {
      "method": "POST",
      "path": "/api/rooms",
      "name": "create_room",
      "weight": 1,
      "body": {
        "name": "TestRoom_{id}",
        "description": "A test chat room {id}",
        "max_clients": 100
      }
    },
    {
      "method": "GET",
      "path": "/api/rooms/{room_id}",
      "name": "get_room_info",
      "weight": 1
    },
    {
      "method": "WEBSOCKET",
      "path": "/api/ws/{room_id}",
      "name": "websocket_chat",
      "weight": 5,
      "username": "TestUser_{id}",
      "messages": [
        {"type": "chat_message", "content": "Hello, room {room_id}!"},
        {"type": "chat_message", "content": "Test message {id}"}
      ]
    }
  ]
}