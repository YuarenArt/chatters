{
  "host": "http://localhost:8080",
  "requests_per_minute": 120,
  "users": 10000,
  "spawn_rate": 50,
  "websocket_message_rate": 10,
  "max_rooms": 1000,
  "max_users_per_room": 1000,
  "endpoints": [
    {
      "method": "GET",
      "path": "/api/health",
      "name": "health_check",
      "weight": 1
    },
    {
      "method": "POST",
      "path": "/api/rooms",
      "name": "create_room",
      "weight": 5,
      "body": {
        "name": "TestRoom_{id}",
        "description": "A test chat room {id}",
        "max_clients": 100
      }
    },
    {
      "method": "GET",
      "path": "/api/rooms/{room_id}",
      "name": "get_room_info",
      "weight": 15
    },
    {
      "method": "WEBSOCKET",
      "path": "/ws/{room_id}",
      "name": "websocket_chat",
      "weight": 40,
      "username": "TestUser_{id}",
      "messages": [
        {
          "type": "chat_message",
          "data": {
            "text": "Hello, room {room_id}!",
            "username": "TestUser_{id}"
          }
        },
        {
          "type": "chat_message",
          "data": {
            "text": "Test message {id}",
            "username": "TestUser_{id}"
          }
        }
      ]
    }
  ]
}
